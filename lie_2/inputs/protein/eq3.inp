[MD]
steps                 10000
stepsize                  1
temperature             150.0
initial_temperature     150.0
random_seed             911
bath_coupling             5
shake_solvent            on
shake_solute            off
lrf                      on

[sphere]
shell_radius 14
shell_force 20 

[cut_offs]

